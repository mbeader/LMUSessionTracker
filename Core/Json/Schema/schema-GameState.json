{
	"$id": "GameState",
	"type": "object",
	"properties": {
		"MultiStintState": {
			"enum": [ "DISABLED", "LOADING", "MONITOR_MENU", "DRIVING", "END_OF_RACE_GRACE", "PAUSED" ]
		},
		"PitEntryDist": {
			"description": "Only present when in session.",
			"type": "number"
		},
		"PitState": {
			"description": "Only present when in session.",
			"type": "string"
		},
		"closeestWeatherNode": {
			"$ref": "schema-WeatherNode.json"
		},
		"gamePhase": {
			"enum": [ "GPHASE_BEFORE", "GPHASE_FORMATION", "GPHASE_GREEN", "GPHASE_CHECKERED" ]
		},
		"haveILoadedAllMyTiresYet": {
			"type": "boolean"
		},
		"inControlOfVehicle": {
			"type": "boolean"
		},
		"inMonitor": {
			"type": "boolean"
		},
		"isReplayActive": {
			"type": "boolean"
		},
		"playerVehicleLoaded": {
			"type": "boolean"
		},
		"raceFinished": {
			"type": "boolean"
		},
		"teamVehicleState": {
			"enum": [ "NO VEHICLE", "VEHICLE AVAILABLE", "WAITING TO GET_IN", "IN CONTROL", "OTHER TEAMMATE DRIVING" ]
		},
		"timeOfDay": {
			"type": "number",
			"minimum": 0
		}
	},
	"additionalProperties": false,
	"required": [
		"MultiStintState",
		"closeestWeatherNode",
		"gamePhase",
		"haveILoadedAllMyTiresYet",
		"inControlOfVehicle",
		"inMonitor",
		"isReplayActive",
		"playerVehicleLoaded",
		"raceFinished",
		"teamVehicleState",
		"timeOfDay"
	]
}
