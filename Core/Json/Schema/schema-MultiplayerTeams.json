{
	"$id": "/LMUSessionTracker/Schema/MultiplayerTeams",
	"$defs": {
		"MultiplayerDriver": {
			"$ref": "/LMUSessionTracker/Schema/MultiplayerDriver"
		},
		"MultiplayerTeam": {
			"$ref": "/LMUSessionTracker/Schema/MultiplayerTeam"
		}
	},
	"type": "object",
	"properties": {
		"coherenceId": {
			"type": "integer"
		},
		"drivers": {
			"description": "Always includes admin.",
			"type": "object",
			"patternProperties": {
				"^.+$": {
					"$ref": "#/$defs/MultiplayerDriver"
				}
			},
			"additionalProperties": false
		},
		"teams": {
			"description": "Number is slot, starting from 0. Hosted starts at some arbitrary value.",
			"type": "object",
			"patternProperties": {
				"^utid[0-9]+$": {
					"$ref": "#/$defs/MultiplayerTeam"
				}
			},
			"additionalProperties": false
		}
	},
	"additionalProperties": false,
	"required": [
		"coherenceId",
		"drivers",
		"teams"
	]
}
