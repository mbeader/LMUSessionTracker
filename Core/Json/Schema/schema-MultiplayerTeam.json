{
	"$id": "/LMUSessionTracker/Schema/MultiplayerTeam",
	"type": "object",
	"properties": {
		"Id": {
			"description": "Team uuid in online events. Integer in online practices.",
			"type": "string",
			"pattern": "^(?:[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12})|(?:[0-9]{1,22})$"
		},
		"carNumber": {
			"type": "string",
			"pattern": "^[0-9]{0,3}$"
		},
		"drivers": {
			"type": "object",
			"patternProperties": {
				"^.+$": {
					"$ref": "/LMUSessionTracker/Schema/MultiplayerTeamMember"
				}
			},
			"additionalProperties": false
		},
		"name": {
			"description": "Limited to 25 characters in online practices.",
			"type": "string"
		},
		"vehicle": {
			"description": "veh file.",
			"type": "string"
		}
	},
	"additionalProperties": false,
	"required": [
		"Id",
		"carNumber",
		"drivers",
		"name",
		"vehicle"
	]
}
