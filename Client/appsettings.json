{
	"Client": {
		"Options": {
			"LMULoggingOnly": false,
			"UseReplay": false,
			"SendReplay": false,
			"PrivateKeyFile": ""
		},
		"LMU": {
			"BaseUri": "http://localhost:6397/",
			"TimeoutSeconds": 2,
			"ValidateResponses": false,
			"LogResponses": false,
			"LogDirectory": "debug"
		},
		"Protocol": {
			"BaseUri": "http://localhost:5000/",
			"TimeoutSeconds": 30
		},
		"Replay": {
			"Directory": "",
			"ValidateResponses": false,
			"Delay": 0
		}
	},
	"SchemaValidation": {
		"Enabled": false
	},
	"Serilog": {
		"Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
			}
		},
		"WriteTo": [
			{
				"Name": "Console"
			},
			{
				"Name": "File",
				"Args": {
					"path": "logs/lmustclient.log",
					"outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{ThreadId}] {Level:u5} {SourceContext} - {Message:lj}{NewLine}{Exception}",
					"rollOnFileSizeLimit": true,
					"fileSizeLimitBytes": 10485760,
					"retainedFileCountLimit": 5
				}
			}
		]
	}
}
